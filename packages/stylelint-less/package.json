{
  "name": "stylelint-less",
  "version": "4.0.0",
  "description": "A collection of Less specific rules for stylelint",
  "keywords": [
    "stylelint",
    "stylelint-plugin",
    "css",
    "less"
  ],
  "homepage": "https://github.com/stylelint-less/stylelint-less#readme",
  "bugs": {
    "url": "https://github.com/stylelint-less/stylelint-less/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/stylelint-less/stylelint-less.git",
    "directory": "packages/stylelint-less"
  },
  "license": "MIT",
  "author": {
    "name": "Sivantarajan",
    "url": "https://github.com/ssivanatarajan/"
  },
  "type": "module",
  "exports": {
    ".": {
      "types": "./index.d.ts",
      "import": "./src/index.js"
    }
  },
  "files": [
    "src/**/*.js",
    "!**/__tests__/*.js",
    "index.d.ts"
  ],
  "scripts": {
    "lint": "tsc --noEmit",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js"
  },
  "lint-staged": {
    "*.{js,mjs,cjs}": [
      "sh -c 'tsc --noEmit'",
      "node --experimental-vm-modules node_modules/jest/bin/jest.js --bail --findRelatedTests"
    ]
  },
  "babel": {
    "presets": [
      "@babel/env"
    ]
  },
  "prettier": "@stylelint/prettier-config",
  "dependencies": {
    "postcss-resolve-nested-selector": "^0.1.1",
    "postcss-value-parser": "4.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.28.6",
    "@babel/preset-env": "^7.28.6",
    "@types/jest": "^30.0.0",
    "@types/postcss-less": "^4.0.7",
    "jest": "^30.2.0",
    "jest-preset-stylelint": "^9.0.0",
    "postcss-less": "^6.0.0",
    "stylelint": "^17.0.0"
  },
  "peerDependencies": {
    "postcss": "^8.4.31",
    "stylelint": "^16.26.1 || ^17.0.0"
  }
}
