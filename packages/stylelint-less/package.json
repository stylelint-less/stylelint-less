{
  "name": "stylelint-less",
  "version": "3.0.1",
  "description": "A collection of Less specific rules for stylelint",
  "keywords": [
    "stylelint",
    "stylelint-plugin",
    "css",
    "less"
  ],
  "homepage": "https://github.com/stylelint-less/stylelint-less#readme",
  "bugs": {
    "url": "https://github.com/stylelint-less/stylelint-less/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/stylelint-less/stylelint-less.git",
    "directory": "packages/stylelint-less"
  },
  "license": "MIT",
  "author": {
    "name": "Sivantarajan",
    "url": "https://github.com/ssivanatarajan/"
  },
  "type": "commonjs",
  "main": "dist/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rimraf dist && rollup -c",
    "prepare": "pnpm run build",
    "prepublishOnly": "pnpm run build",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js"
  },
  "lint-staged": {
    "*.{js,mjs,cjs}": "node --experimental-vm-modules node_modules/jest/bin/jest.js --bail --findRelatedTests"
  },
  "babel": {
    "presets": [
      "@babel/env"
    ]
  },
  "prettier": "@stylelint/prettier-config",
  "dependencies": {
    "postcss-resolve-nested-selector": "^0.1.1",
    "postcss-value-parser": "4.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.23.6",
    "@babel/preset-env": "^7.23.9",
    "@rollup/plugin-commonjs": "^29.0.0",
    "@rollup/plugin-node-resolve": "^16.0.3",
    "jest": "^30.2.0",
    "jest-preset-stylelint": "^8.0.0",
    "postcss-less": "^6.0.0",
    "rimraf": "^6.1.2",
    "rollup": "^4.10.0",
    "stylelint": "^16.0.2"
  },
  "peerDependencies": {
    "postcss": "^8.4.31",
    "stylelint": "^16.0.2"
  }
}
