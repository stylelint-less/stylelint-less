{
  "name": "stylelint-config-standard-less",
  "version": "4.0.1",
  "description": "The standard Less config for Stylelint",
  "keywords": [
    "stylelint",
    "stylelint-config",
    "standard",
    "less"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/stylelint-less/stylelint-less.git",
    "directory": "packages/stylelint-config-standard-less"
  },
  "license": "MIT",
  "author": "Crazy Urus <crazyurus@vip.qq.com>",
  "type": "module",
  "exports": {
    ".": {
      "types": "./index.d.ts",
      "import": "./index.js"
    }
  },
  "files": [
    "index.js",
    "index.d.ts"
  ],
  "scripts": {
    "lint": "tsc --noEmit"
  },
  "lint-staged": {
    "*.{js,cjs,mjs}": "sh -c 'tsc --noEmit'"
  },
  "dependencies": {
    "stylelint-config-recommended-less": "workspace:^",
    "stylelint-config-standard": "^40.0.0"
  },
  "peerDependencies": {
    "postcss": "^8.3.3",
    "stylelint": "^16.26.1 || ^17.0.0"
  },
  "peerDependenciesMeta": {
    "postcss": {
      "optional": true
    }
  }
}
